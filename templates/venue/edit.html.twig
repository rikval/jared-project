{% extends 'base.html.twig' %}

{% block title %}{{ parent() }} - edit: {{ venue.name }}{% endblock %}

{% block body %}
    <h1>edit a venue</h1>

    {{ form_start(form) }}
    {{ form_row(form.name) }}
    {{ form_row(form.location) }}
    {{ form_row(form.audienceCapacity) }}
    {{ form_row(form.note) }}

    <div class="text-right">
        <button type="submit" class="btn btn-primary">
            edit
        </button>
    </div>
    {{  form_end(form) }}
{% endblock %}