{% extends 'base.html.twig' %}

{% block title %}{{ parent() }} - {{ artist.name }}{% endblock %}

{% block body %}
    <h1>Edit {{ artist.name }}</h1>

    {{ form_start(form) }}
    {{ form_row(form.name) }}

    <div class="text-right">
        <button type="submit" class="btn btn-success">
            Edit Artist
        </button>
    </div>
    {{  form_end(form) }}
{% endblock %}